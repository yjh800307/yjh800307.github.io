[Unit]
Description=Aria2 Download Manager
After=network.target

[Service]
Type=forking
ExecStart=/usr/bin/aria2c --conf - path=/home/user/.aria2/aria2.conf
ExecReload=/bin/kill -HUP $MAINPID
KillMode=process
Restart=on - failure

[Install]
WantedBy=multi - user.target
